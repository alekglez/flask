{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "flask-app",
      "image": "alekcoraglez/flask-app",
      "hostname": "flask-app",
      "essential": true
    },
    {
      "name": "nginx",
      "image": "alekcoraglez/flask-app-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 8081,
          "containerPort": 8081
        }
      ]
    }
  ]
}
